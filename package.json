{
  "name": "circu-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "scripts": {
    "editor:dev": "pnpm --filter circu-editor vite",
    "editor:dev:huge": "pnpm --filter circu-editor vite --mode huge-document",
    "editor:dev:empty": "pnpm --filter circu-editor vite --mode empty",
    "editor:build": "pnpm --filter circu-editor vite build",
    "editor:build:huge": "pnpm --filter circu-editor vite build --mode huge-document",
    "editor:preview": "pnpm --filter circu-editor pnpm vite preview",
    "wiki:dev": "pnpm --filter circu-wiki vite",
    "wiki:build": "pnpm --filter circu-wiki vite build",
    "wiki:preview": "pnpm --filter circu-wiki vite preview",
    "server:prebuild": "pnpm --filter circu-server rimraf dist",
    "server:build": "pnpm --filter circu-server nest build",
    "server:format": "pnpm --filter circu-server prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "server:start": "pnpm --filter circu-server nest start",
    "server:start:dev": "pnpm --filter circu-server nest start --watch",
    "server:start:debug": "pnpm --filter circu-server nest start --debug --watch",
    "server:start:prod": "pnpm --filter circu-server node dist/main",
    "server:lint": "pnpm --filter circu-server eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "server:test": "pnpm --filter circu-server jest",
    "server:test:watch": "pnpm --filter circu-server jest --watch",
    "server:test:cov": "pnpm --filter circu-server jest --coverage",
    "server:test:debug": "pnpm --filter circu-server node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "pnpm --filter circu-server jest --config ./test/jest-e2e.json",
    "format": "pnpm prettier --write ."
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "typescript": "^4.8.2"
  },
  "keywords": [],
  "author": "Flrande",
  "license": "ISC"
}
